\begin_layout Subsection*
Core Components
\end_layout

\begin_layout Standard
As shown in Figure
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand ref
reference "fig:mc_arch"
plural "false"
caps "false"
noprefix "false"

\end_inset

, the essential modules include:
\end_layout

\begin_layout Itemize

\series bold
Processor Core
\series default
: ARM/RISC-V/AVR architectures 
\end_layout

\begin_layout Itemize

\series bold
Memory
\series default
: 
\end_layout

\begin_deeper
\begin_layout Itemize
SRAM: 
\begin_inset Formula $8-256$
\end_inset

KB for runtime data 
\end_layout

\begin_layout Itemize
Flash: 
\begin_inset Formula $32-2048$
\end_inset

KB for firmware 
\end_layout

\end_deeper
\begin_layout Itemize

\series bold
Timers
\series default
: 16-bit PWM generation (e.g., 
\family typewriter
TIM1
\family default
 in STM32) 
\end_layout

