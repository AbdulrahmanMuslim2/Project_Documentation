\begin_layout Subsection*
Sensor Integration Examples
\end_layout

\begin_layout Itemize

\series bold
IMU
\series default
: MPU6050 (I2C) for attitude estimation 
\end_layout

\begin_layout Itemize

\series bold
Barometer
\series default
: MS5611 (SPI) for altitude hold 
\end_layout

\begin_layout Itemize

\series bold
RC Receiver
\series default
: PPM signal capture via timer input 
\end_layout

\begin_layout Subsubsection
Timer/Counter Modules
\end_layout

\begin_layout Standard
\begin_inset CommandInset label
LatexCommand label
name "sec:timers"

\end_inset


\end_layout

\begin_layout Subsection*
Core Architecture
\end_layout

\begin_layout Standard
As shown in Figure
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand ref
reference "fig:mc_arch"
plural "false"
caps "false"
noprefix "false"

\end_inset

, timers consist of:
\end_layout

\begin_layout Itemize

\series bold
Counter Register
\series default
: Stores current value (16/32-bit) 
\end_layout

\begin_layout Itemize

\series bold
Prescaler
\series default
: Divides clock frequency (configurable) 
\end_layout

\begin_layout Itemize

\series bold
Auto-Reload Register
\series default
: Defines periodicity 
\end_layout

