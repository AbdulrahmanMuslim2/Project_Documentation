\begin_layout Subsection*
PWM-to-Analog Conversion
\end_layout

\begin_layout Itemize
Low-pass filter design: 
\begin_inset Formula 
\begin{equation}
f_{c}=\frac{1}{2\pi RC}\quad(f_{c}\ll f_{\text{PWM}})
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
Typical values: 
\begin_inset Formula $R=1k\Omega$
\end_inset

, 
\begin_inset Formula $C=10\mu F$
\end_inset

 for 100Hz PWM 
\end_layout

