\begin_layout Part
Control and Simulation
\end_layout

\begin_layout Chapter
Flight Simulator
\end_layout

\begin_layout Section
Airframe Model
\end_layout

\begin_layout Standard
We used the stability derivatives resulting from AVL to implement the first
 order taylor series approximation of the airframe model of the missile:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccc}
\Delta X & = & X_{u}u+X_{w}w+X_{\delta_{e}}\delta_{e}+X_{\delta_{th}}\delta_{th}\\
\Delta Y & = & Y_{v}v+Y_{p}p+Y_{r}r+Y_{\delta_{a}}\delta_{a}+Y_{\delta_{r}}\delta_{r}\\
\Delta Z & = & Z_{u}u+Z_{w}w+Z_{\dot{w}}\dot{w}+Z_{q}q+Z_{\delta_{e}}\delta_{e}+Z_{\delta_{th}}\delta_{th}\\
\Delta L & = & L_{v}v+L_{p}p+L_{r}r+L_{\delta_{a}}\delta_{a}+L_{\delta_{r}}\delta_{r}\\
\Delta M & = & M_{u}u+M_{w}w+M_{\dot{w}}\dot{w}+M_{q}q+M_{\delta_{e}}\delta_{e}+M_{\delta_{th}}\delta_{th}\\
\Delta N & = & N_{v}v+N_{p}p+N_{r}r+N_{\delta_{a}}\delta_{a}+N_{\delta_{r}}\delta_{r}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Section
Dimensionalization
\end_layout

\begin_layout Standard
The output stability derivatives of AVL software are actually the coefficients
 and not the real derivatives, so this needed to go through the dimensionalizati
on process, this was done using 
\begin_inset CommandInset citation
LatexCommand cite
key "nacaCR2144,taha_advanced_mechanics_2023"
literal "true"

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $X_{U}=\frac{1}{2m}\rho U_{o}^{2}S_{ref}\frac{\partial C_{X}}{\partial u}=\frac{1}{2m}\rho U_{o}S_{ref}C_{X_{U}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X_{W}=\frac{1}{2m}\rho U_{o}^{2}S_{ref}\frac{\partial C_{X}}{\partial w}=\frac{1}{2m}\rho U_{o}S_{ref}C_{X_{w}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
X_{\delta_{e}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{X}}{\partial\delta_{e}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}C_{X_{\delta_{e}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $Z_{U}=\frac{1}{2m}\rho U_{o}^{2}S_{ref}\frac{\partial C_{Z}}{\partial u}=\frac{1}{2m}\rho U_{o}S_{ref}C_{Z_{U}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{W}=\frac{1}{2m}\rho U_{o}^{2}S_{ref}\frac{\partial C_{Z}}{\partial w}=\frac{1}{2m}\rho U_{o}S_{ref}C_{Z_{w}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{Q}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Z}}{\partial q}=\frac{1}{4m}\rho V_{To}S_{ref}\bar{C}C_{Z_{q}}\qquad\left[m.rad^{-1}s^{-1}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{\delta_{e}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Z}}{\partial\delta_{e}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}C_{Z_{\delta_{e}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $M_{U}=\frac{1}{2I_{yy}}\rho U_{o}^{2}S_{ref}\bar{C}\frac{\partial C_{M}}{\partial u}=\frac{1}{2I_{yy}}\rho U_{o}S_{ref}\bar{C}C_{M_{u}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
M_{W}=\frac{1}{2I_{yy}}\rho U_{o}^{2}S_{ref}\bar{C}\frac{\partial C_{M}}{\partial w}=\frac{1}{2I_{yy}}\rho U_{o}S_{ref}\bar{C}C_{M_{w}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
M_{Q}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}\bar{C}\frac{\partial C_{M}}{\partial q}=\frac{1}{4I_{yy}}\rho V_{To}S_{ref}\bar{C}^{2}C_{M_{q}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
M_{\delta_{e}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}\bar{C}\frac{\partial C_{M}}{\partial\delta_{e}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}\bar{C}C_{M_{\delta_{e}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $Y_{V}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Y}}{\partial v}=\frac{1}{2m}\rho V_{To}S_{ref}C_{Y_{v}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y_{P}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Y}}{\partial p}=\frac{1}{4m}\rho V_{To}S_{ref}bC_{Y_{p}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y_{R}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Y}}{\partial r}=\frac{1}{4m}\rho V_{To}S_{ref}bC_{Y_{r}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y_{\delta_{a}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Y}}{\partial\delta_{a}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}C_{Y_{\delta_{a}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula 
\[
Y_{\delta_{r}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}\frac{\partial C_{Y}}{\partial\delta_{r}}=\frac{1}{2m}\rho V_{To}^{2}S_{ref}C_{Y_{\delta_{r}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $L_{V}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{L}}{\partial V}=\frac{1}{2I_{xx}}\rho V_{To}S_{ref}bC_{L_{p}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{P}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{L}}{\partial p}=\frac{1}{4I_{xx}}\rho V_{To}S_{ref}b^{2}C_{L_{p}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{r}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{L}}{\partial r}=\frac{1}{4I_{xx}}\rho V_{To}S_{ref}b^{2}C_{L_{r}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{\delta_{a}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{L}}{\partial\delta_{a}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}bC_{L_{\delta_{a}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{\delta_{r}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{L}}{\partial\delta_{r}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}bC_{L_{\delta_{r}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $N_{V}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{N}}{\partial V}=\frac{1}{2I_{xx}}\rho V_{To}S_{ref}bC_{N_{p}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{P}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{N}}{\partial p}=\frac{1}{4I_{xx}}\rho V_{To}S_{ref}b^{2}C_{N_{p}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{r}=\frac{1}{2I_{xx}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{N}}{\partial r}=\frac{1}{4I_{xx}}\rho V_{To}S_{ref}b^{2}C_{N_{r}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{\delta_{a}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{N}}{\partial\delta_{a}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}bC_{N_{\delta_{a}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{\delta_{r}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}b\frac{\partial C_{N}}{\partial\delta_{r}}=\frac{1}{2I_{yy}}\rho V_{To}^{2}S_{ref}bC_{N_{\delta_{r}}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{V}^{'}=G\left(L_{V}+\frac{I_{xz}}{I_{xx}}N_{V}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{P}^{'}=G\left(L_{P}+\frac{I_{xz}}{I_{xx}}N_{P}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{R}^{'}=G\left(L_{R}+\frac{I_{xz}}{I_{xx}}N_{R}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{\delta_{a}}^{'}=G\left(L_{\delta_{a}}+\frac{I_{xz}}{I_{xx}}N_{\delta_{a}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
L_{\delta_{r}}^{'}=G\left(L_{\delta_{r}}+\frac{I_{xz}}{I_{xx}}N_{\delta_{r}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{V}^{'}=G\left(N_{V}+\frac{I_{xz}}{I_{zz}}L_{V}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{P}^{'}=G\left(N_{P}+\frac{I_{xz}}{I_{zz}}L_{P}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{R}^{'}=G\left(N_{R}+\frac{I_{xz}}{I_{zz}}L_{R}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{\delta_{a}}^{'}=G\left(N_{\delta_{a}}+\frac{I_{xz}}{I_{zz}}L_{\delta_{a}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{\delta_{r}}^{'}=G\left(N_{\delta_{r}}+\frac{I_{xz}}{I_{zz}}L_{\delta_{r}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $G=\frac{I_{xx}I_{zz}}{I_{xx}I_{zz}-I_{xz}^{2}}=\frac{1}{1-\frac{I_{xz}^{2}}{I_{xx}I_{zz}}}$
\end_inset

.
\end_layout

\begin_layout Chapter
Oblate Rotating Earth 6-DoF Rigid Body Simulator
\end_layout

\begin_layout Section
The Shape of the Earth, WGS-84
\end_layout

\begin_layout Standard
Simulation of high-speed flight over large areas of Earths surface, with
 accurate equations of motion and precise calculation of position, requires
 an accurate model of Earths shape, rotation, and gravity.
 Meridional cross sections of Earth are approximately elliptical and the
 polar radius of Earth is about 21 km less than the equatorial radius, so
 the solid figure generated by rotating an appropriately scaled ellipse
 about its minor axis will provide a model of Earths shape.
 Organizations from many countries participate in making accurate measurements
 of the parameters of these spheroidal (i.e., ellipsoids of revolution) models.
 In the United States the current model is the Department of Defense World
 Geodetic System 1984, or WGS-84, and the agency responsible for supporting
 this model is the National Imagery and Mapping Agency(NIMA,1997).
 The Global Positioning System(GPS)reliesonWGS-84for the ephemerides of
 its satellites.
\end_layout

\begin_layout Standard
The equipotential surface of Earths gravity field that coincides with the
 undisturbed mean sea level extended continuously underneath the continents
 is called the 
\series bold
geoid
\series default
.
 Earths irregular mass distribution causes the geoid to be an undulating
 surface, and this is illustrated in 
\begin_inset CommandInset ref
LatexCommand formatted
reference "fig:The-geoid-and"
plural "false"
caps "false"
noprefix "false"

\end_inset

.
 Note that the local vertical is defined by the direction in which a plumb-bob
 hangs and is accurately normal to the geoid.
 The angle that it makes with the spheroid normal is called the deflection
 of the vertical and is usually less than 10 arc-s (the largest deflections
 over the entire Earth are about1 arc-min).
 
\begin_inset CommandInset ref
LatexCommand formatted
reference "fig:The-oblate-spheroidal"
plural "false"
caps "false"
noprefix "false"

\end_inset

 shows the Earth spheroid, with the oblateness greatly exaggerated.The coordinate
 system shown has its origin at Earths center of mass(indirectly determined
 from satellite orbits), z up the spin axis, and its x and y axes in the
 equatorial plane.
\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename flightSimulator/Users/podyd/OneDrive/Pictures/Screenshots/Screenshot 2025-06-12 193606.png
	width 50page%

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
The geoid and definitions of height.
\begin_inset CommandInset label
LatexCommand label
name "fig:The-geoid-and"

\end_inset


\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename flightSimulator/Users/podyd/OneDrive/Pictures/Screenshots/Screenshot 2025-06-12 193732.png
	width 50page%

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
The oblate spheroidal model of the Earth.
\begin_inset CommandInset label
LatexCommand label
name "fig:The-oblate-spheroidal"

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Standard
Based on a 
\begin_inset Formula $1^{\circ}*1^{\circ}$
\end_inset

 (latitude, longitude) worldwide grid, the root-mean-square(rms)deviation
 of the geoid from the spheroid is only about 30m! The WGS-84 defined and
 derived values are:
\end_layout

\begin_layout Standard
\begin_inset Formula $f\equiv flatness=1∕298.257223563$
\end_inset

, 
\begin_inset Formula $a\equiv\text{semi-major axis}=6378137.0\;m$
\end_inset

, 
\begin_inset Formula $e\equiv\text{eccentricity}=.0818191908426$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $GM=\mu\equiv\text{Gravitational Parameter}=3986004.418\times10^{8}\;m^{3}/s^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\omega_{E}=(\frac{2𝜋}{243600})*(1+1∕365.25)=7.2921150\times10^{-5}\;rad/s$
\end_inset

, neglecting the inclination of Earths axis due to earth's precession around
 its Z axis).
\end_layout

\begin_layout Section
Gravitation and Gravity
\end_layout

\begin_layout Standard
The term gravitation denotes a mass attraction effect, as distinct from
 gravity, meaning the combination of mass attraction and centrifugal force
 experienced by a body constrained to move with Earths surface.
\end_layout

\begin_layout Standard
The WGS-84 datum includes an amazingly detailed model of Earths gravitation.
 This model is in the form of a (scalar) potential function, V, such that
 components of specific mass attraction force along each of three axes can
 be found from the respective gradients of the potential function.
 The current potential function, for use with 
\series bold
WGS-84
\series default
, is Earth Gravitational Model 1996 (
\series bold
EGM96
\series default
).
 This has 
\begin_inset Formula $130,676$
\end_inset

 coefficients and is intended for very precise satellite and missile calculation
s.
 The largest coefficient is two orders of magnitude bigger than the next
 coefficient and, if we retain only the largest coefficient, the result
 is still a very accurate model.
 Neglecting the other coefficients removes the dependence on terrestrial
 longitude, leaving the following potential function at point 
\begin_inset Formula $P$
\end_inset

(
\begin_inset Formula $r$
\end_inset

,
\begin_inset Formula $\psi$
\end_inset

).
\end_layout

\begin_layout Standard
The constant J2 is given by
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
J_{2}=-\sqrt{5}\bar{C}_{2,0}=1.082626684\times10^{-3}
\]

\end_inset


\end_layout

\begin_layout Standard
Where 
\begin_inset Formula $\bar{C}_{2,0}$
\end_inset

is the actual EGM96 coefficient.
 
\end_layout

\begin_layout Standard
The gradients of the potential function are easily evaluated in geocentric
 coordinates.
 When this is done and the results are transformed into the ECEF system,
 we obtain the following gravitation model for the gravitational acceleration,
 G:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
G^{ecef}=-\frac{GM}{r^{2}}\begin{bmatrix}[1+\frac{3}{2}(\frac{a}{r})^{2}J_{2}(1-5sin^{2}\psi)]p_{x}/r\\{}
[1+\frac{3}{2}(\frac{a}{r})^{2}J_{2}(1-5sin^{2}\psi)]p_{y}/r\\{}
[1+\frac{3}{2}(\frac{a}{r})^{2}J_{2}(3-5sin^{2}\psi)]p_{z}/r
\end{bmatrix}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
where the geocentric position vector is p, with ECEF components 
\begin_inset Formula $p_{x}$
\end_inset

, 
\begin_inset Formula $p_{y}$
\end_inset

, 
\begin_inset Formula $p_{z}$
\end_inset

 and length r, and the geocentric latitude is given by 
\begin_inset Formula $\sin\psi=p_{z}∕r$
\end_inset

.
 This model is accurate to about 
\begin_inset Formula $30*10^{-3}$
\end_inset

 to 
\begin_inset Formula $35*10^{-3}$
\end_inset

 
\begin_inset Formula $cm∕s^{2}$
\end_inset

 rms, but local deviations can be quite large.
 Note that the x- and y-components are identical because there is no longitude
 dependence.
 The model can also be converted to geodetic coordinates using the relationships
 given earlier.The weight of an object on Earth is determined by the gravitationa
l attraction (mG) minus the mass times the centripetal acceleration needed
 to produce the circular motion in inertial space at geocentric position
 vector p.
 Dividing the weight of the object by its mass gives the gravity vector
 g.
 Therefore, the vector equation for g is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
g=G-\omega_{e/i}\times\left(\omega_{e/i}\times p\right)
\]

\end_inset


\end_layout

\begin_layout Standard
The down component of 
\begin_inset Formula $g$
\end_inset

 given by the model, at Earths surface, varies sinusoidally from 
\begin_inset Formula $9.780\;m/s^{2}$
\end_inset

 at the equator to 
\begin_inset Formula $9.806\;m/s^{2}$
\end_inset

 at 
\begin_inset Formula $45^{\circ}$
\end_inset

 geodetic latitude and 9.832 
\begin_inset Formula $m/s^{2}$
\end_inset

 at the poles.
\end_layout

\begin_layout Section

\size large
Quaternions
\end_layout

\begin_layout Standard

\size large
Quaternions introduced here because of their “all-attitude” capability and
 numerical advantages in simulation and control.
 They are now widely used in simulation, robotics, guidance and navigation
 calculations, attitude control, and graphics animation.
 We will review enough of their properties to use them for coordinate rotation.
\end_layout

\begin_layout Standard

\size large
Quaternions, a complex number algebra invented by Sir William Rowan Hamilton
 in 1843 in an attempt to generalize ordinary complex numbers to three dimension
s.
 These quaternion state equations have additional numerical advantages over
 the all other common methods, they also avoid the gimball lock problem
 that appears in euler angles rotations for example.
\end_layout

\begin_layout Subsection
Quaternion Properties
\end_layout

\begin_layout Standard
if 
\begin_inset Formula $p$
\end_inset

 and 
\begin_inset Formula $q$
\end_inset

 are two quaternions, then:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
norm\left(q\right)=\sum_{i=0}^{i=3}q_{i}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p*q\neq q*p
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q^{-1}=\frac{1}{norm\left(q\right)}\begin{bmatrix}q_{0}\\
-\boldsymbol{q_{1,2,3}}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q_{ab}=q_{ba}^{-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(p*q\right)^{-1}=q^{-1}*p^{-1}
\]

\end_inset


\end_layout

\begin_layout Standard
If the direction angles of the axis are 
\begin_inset Formula $\alpha$
\end_inset

, 
\begin_inset Formula $\beta$
\end_inset

, 
\begin_inset Formula $\gamma$
\end_inset

 and a measure of the rotation angle is 
\begin_inset Formula $\delta$
\end_inset

, the rotation quaternion is written as:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q=\begin{bmatrix}\cos\delta\\
\cos\alpha\sin\delta\\
\cos\beta\sin\delta\\
\cos\gamma\sin\delta
\end{bmatrix}=\begin{bmatrix}\cos\delta\\
\sin\delta\boldsymbol{n^{r}}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Subsection
Vector Rotation by Quaternions
\end_layout

\begin_layout Standard

\size large
A quaternion can be used to rotate a Euclidean vector in the same manner
 as the rotation formula, and the quaternion rotation is much simpler in
 form.
 The vector part of the quaternion is used to define the rotation axis and
 the scalar part to define the angle of rotation.
 The rotation axis is specified by its direction cosines in the reference
 coordinate system, and it is convenient to impose a unity norm constraint
 on the quaternion, this is usually done in a simulator.
\end_layout

\begin_layout Standard
This is the equation for vector rotation using quaternions:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
v=q*u*q^{-1}
\]

\end_inset


\end_layout

\begin_layout Subsection
Direction Cosine Matrix from Quaternion
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
C_{b/a}=\begin{bmatrix}(q_{0}^{2}+q_{1}^{2}-q_{2}^{2}-q_{3}^{2}) & 2(q_{1}q_{2}+q_{0}q_{3}) & 2(q_{1}q_{3}-q_{0}q_{2})\\
2(q_{1}q_{2}-q_{0}q_{3}) & (q_{0}^{2}-q_{1}^{2}+q_{2}^{2}-q_{3}^{2}) & 2(q_{2}q_{3}+q_{0}q_{1})\\
2(q_{1}q_{3}+q_{0}q_{2}) & 2(q_{2}q_{3}-q_{0}q_{1}) & (q_{0}^{2}-q_{1}^{2}-q_{2}^{2}+q_{3}^{2})
\end{bmatrix}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
This shall be used to get DCM from quaternion 
\begin_inset Formula $q_{b/n}$
\end_inset

, in order to calculate the euler angles, using the equations:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccc}
\phi & = & atan2\left(c_{23},c_{33}\right)\\
\theta & = & -asin\left(c_{13}\right)\\
\psi & = & atan2\left(c_{12},c_{11}\right)
\end{array}
\]

\end_inset


\end_layout

\begin_layout Subsection
Quaternion Initialization
\end_layout

\begin_layout Standard
Now a few other quaternions need to be calculated in order to fully implement
 the simulator.
\end_layout

\begin_layout Standard
Given initial conditions of attitude (euler angles), the quaternion 
\begin_inset Formula $q_{b/n}$
\end_inset

 can be calculated to relate the body frame to the navigation NED frame.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q_{b/n}=q_{\psi}*q_{\theta}*q_{\psi}
\]

\end_inset


\end_layout

\begin_layout Standard
where you can find each 
\begin_inset Formula $q$
\end_inset

 using the formula:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q_{\mu}\left(t\right)=\begin{bmatrix}\cos\frac{\mu}{2}\\
\sin\frac{\mu}{2}\boldsymbol{n^{r}}
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
You 
\end_layout

\begin_layout Standard
Now to relate the NED frame to the ECEF frame:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
q_{n/e}=q_{l}*q_{\left(-90^{\circ}-\phi\right)}
\]

\end_inset


\end_layout

\begin_layout Section
The Oblate Rotating Earth 6-DoF Equations
\end_layout

\begin_layout Standard
Our differential equations of motion are to be written and integrated in
 the ECEF coordinate system as follows
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{bmatrix}\dot{q}_{b/e} & = & \frac{1}{2}q_{b/e}\left(\omega_{b/i}^{b}-\omega_{e/i}^{b}\right)\\
\dot{P}^{e} & = & v^{e}\\
\dot{v}^{e} & = & \frac{F^{e}}{m}-2\omega_{e/i}^{e}\times v^{e}+g^{e}\\
\omega_{b/i}^{b} & = & \left(J^{b}\right)^{-1}\left(M^{b}-\omega_{b/i}^{b}\times\left(J^{b}\omega_{b/i}^{b}\right)\right)
\end{bmatrix}\label{eq:Oblate 6DOF Simulator}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Where the subscripts/superscripts 
\begin_inset Formula $b$
\end_inset

, 
\begin_inset Formula $e$
\end_inset

 and 
\begin_inset Formula $i$
\end_inset

 mean body, ECEF and Inertial frames respectively, with these choices of
 coordinate systems the state vector, for the set of 6-DoF equations, will
 be
\end_layout

\begin_layout Standard
\align center
\begin_inset Formula $X=\begin{bmatrix}q_{ref/ecf} & p_{cm/O}^{ecf} & v_{cm/e}^{ecf} & \omega_{b/i}^{frd}\end{bmatrix}^{T}$
\end_inset


\end_layout

\begin_layout Standard
The following auxiliary equations must be executed first to compute Earths
 angular velocity in body-fixed coordinates for the quaternion equation
 and the aerodynamic forces in Earth-fixed coordinates for the velocity
 state equation:
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{equation}
\backslash
begin{bmatrix}
\end_layout

\begin_layout Plain Layout

0 
\backslash

\backslash

\end_layout

\begin_layout Plain Layout


\backslash
omega_{e/i}^{frd}
\end_layout

\begin_layout Plain Layout


\backslash
end{bmatrix}=q_{frd/ecf}^{-1}*
\end_layout

\begin_layout Plain Layout


\backslash
begin{bmatrix}
\end_layout

\begin_layout Plain Layout

0 
\backslash

\backslash

\end_layout

\begin_layout Plain Layout


\backslash
omega_{e/i}^{ecf}
\end_layout

\begin_layout Plain Layout


\backslash
end{bmatrix}*q_{frd/ecf}
\backslash
quad
\end_layout

\begin_layout Plain Layout


\backslash
begin{bmatrix}
\end_layout

\begin_layout Plain Layout

0 
\backslash

\backslash

\end_layout

\begin_layout Plain Layout


\backslash
frac{F^{ecf}}{
\backslash
mathrm{m}}
\end_layout

\begin_layout Plain Layout


\backslash
end{bmatrix}=q_{frd/ecf}*
\end_layout

\begin_layout Plain Layout


\backslash
begin{bmatrix}
